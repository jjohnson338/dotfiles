Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

XTerm*background: #e0e0e0
XTerm*foreground: #2c2c2c
XTerm*pointerColor: #BF55EC
XTerm*faceName: Fixed
XTerm*faceSize: 10
XTerm*selectToClipboard: true

! State:            'bg'        'fg'        'bgalt'     'hlbg'      'hlfg'
rofi.color-normal   #BF55EC     #FFFFFF     #BF55EC     #c87de8     #000000
rofi.color-window   #BF55EC     #BF55EC     #000000